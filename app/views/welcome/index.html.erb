
<!-- Header and Nav -->

  <div class="row">
    <div class="large-3 columns">
      <h1><%= image_tag('g3view.png', id: 'logo') %></h1>
    </div>
    <div class="large-9 columns">
      <ul class="inline-list right">
        <li><%= link_to "Account", @user %></li>
        <li><a href="/logout">Sign Out</a></li>
      </ul>
    </div>
  </div>

  <!-- End Header and Nav -->

  <div class="row">

    <!-- Main Content Section -->
    <!-- This has been source ordered to come first in the markup (and on small devices) but to be to the right of the nav on larger screens -->
    <div class="large-9 push-3 columns">

      <h3>gitThreeView <small>a git visualization tool</small></h3>

      <div id="graph-canvas"></div>
    </div>


    <!-- Nav Sidebar -->
    <!-- This is source ordered to be pulled to the left on larger screens -->
    <div class="large-3 pull-9 columns">

      <ul class="side-nav">
      <% @repos.each do |repo| %>
        <li><%= repo.title %><%= link_to "Repo" + repo.id,  repo %></li>
      <% end %>
        <li><a href="#">Repo 2</a></li>
        <li><a href="#">Repo 3</a></li>
        <li><a href="#">Repo 4</a></li>
        <li><a href="#">Repo 5</a></li>
        <li><a href="#">Repo 6</a></li>

      </ul>

    </div>

  </div>


  <!-- Footer -->

  <footer class="row">
    <div class="large-12 columns">
      <hr />
      <div class="row">
        <div class="large-6 columns">
          <p></p>
        </div>
        <div class="large-6 columns">
          <ul class="inline-list right">
            <li><a href="http://threejs.org/">Made with Three.js</a></li>
            <li><a href="http://github.com/ajkamel">About me</a></li>
          </ul>
        </div>
      </div>
    </div>
  </footer>

